x = 0.1:0.2:100;
y = 2 * x.^2;

figure;
subplot(2,2,1);
plot(x,y);
title('plot linear');
grid on;

subplot(2,2,2);
semilogx(x,y);
title('plot semilogx');
grid on;

subplot(2,2,3);
semilogy(x,y);
title('plot semilogy');
grid on;

subplot(2,2,4); 
loglog(x,y); 
title('plot loglog');
grid on;
saveas(gcf,'log_plots.png');
